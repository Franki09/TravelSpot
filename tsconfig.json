{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "app/details/[id].jsx",
    "app/_layout.jsx",
    "app/(tabs)/_layout.jsx",
    "app/(tabs)/about.jsx",
    "app/(tabs)/favorites.jsx",
    "app/(tabs)/index.jsx"
  ]
}
